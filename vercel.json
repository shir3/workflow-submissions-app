{
  "version": 2,
  "builds": [
    {
      "src": "server/proxy-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/proxy-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ],
  "installCommand": "yarn install",
  "buildCommand": "yarn build"
}
